{
  "name": "wok.js",
  "version": "1.1.0",
  "description": "Minimalist frontend framework.",
  "main": "wok.js",
  "bin": {
    "npm run wokinit": "./init.js"
  },
  "preferGlobal": true,
  "scripts": {
    "fry": "node newWok.js",
    "wok": "concurrently \" node wok.js && npm run live\" \"npm run observer\"",

    "live": "live-server --port=9000 --entry-file=./_build/index.html",
    "observer": "nodemon --watch ./_src --ext html,js,css wok.js",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Vasvary Botond",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^7.6.0",
    "escodegen": "^2.0.0",
    "live-server": "^1.2.2",
    "nodemon": "^2.0.20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dejavu333/wok.js.git"
  },
  "bugs": {
    "url": "https://github.com/Dejavu333/wok.js/issues"
  },
  "homepage": "https://github.com/Dejavu333/wok.js#readme"
}
